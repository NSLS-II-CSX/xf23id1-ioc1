file "db/motor.db"
{

# ADDR ---> axis #
# NOTE ---> 2 asyn ports: 1) lowel comms ("P0"), and 2) Motor Record ("motor")
pattern
{ P,             M,               MOTOR,   PORT,    ADDR, DESC,     DTYP }
{ "Pri-Sys", "{Dev-Ax:X}", 	  "motor", "PO",    1,  "x-axis", asynMotor }
{ "Pri-Sys", "{Dev-Ax:Y}",    "motor", "PO",    2,  "y-axis", asynMotor }
}
