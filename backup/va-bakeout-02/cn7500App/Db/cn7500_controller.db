## Database for Omega CN7500 temperature controller
## Substitutions:
## Pri		-	Primary system
## Sec		-	Secodary system, including serial number
## Dev		-	Device name
## HexAddr	-	Unit address
## PORT		-	IP or Serial port name

# Get process value
record(ai, "$(Sys)$(Dev)T-I")
{
	field(DTYP, "stream")
	field(DESC, "Process Value")
	field(INP,  "@cn7500.proto read_reg($(HexAddr),1000) $(PORT)")
	field(LINR, "SLOPE")
	field(ESLO, "0.1")
	field(PREC, "1")
	field(EGU,  "Celsius")
	field(SCAN, "1 second")
	field(LSV,  "NO_ALARM")
	field(HSV,  "MINOR")
	field(HHSV, "MAJOR")
	field(HIGH, "160")
	field(HIHI, "210")
}

# Set/Get upper-limit of temperature range
record(ao, "$(Sys)$(Dev)Sys:HiT-SP")
{
	field(DTYP, "stream")
	field(DESC, "Set T Hi Limit")
	field(OUT,  "@cn7500.proto write_reg($(HexAddr),1002) $(PORT)")
	field(DRVH, "600")
	field(DRVL, "0")	
	field(LINR, "SLOPE")
	field(ESLO, "0.1")
	field(PREC, "1")
	field(EGU,  "Celsius")
	field(ASG,  "VAWRITE")
}

record(ai, "$(Sys)$(Dev)Sys:HiT-RB")
{
	field(DTYP, "stream")
	field(DESC, "T Hi Limit")
	field(INP,  "@cn7500.proto read_reg($(HexAddr),1002) $(PORT)")
	field(PINI, "1")
	field(LINR, "SLOPE")
	field(ESLO, "0.1")
	field(PREC, "1")
	field(EGU,  "Celsius")
	field(SCAN, "10 second")
	field(FLNK, "$(Sys)$(Dev)Scan:1-FOut_")
}

record(fanout, "$(Sys)$(Dev)Scan:1-FOut_")
{
	field(DESC, "Scan Fanout 1")
	field(LNK1, "$(Sys)$(Dev)Sys:LoT-RB")
	field(LNK2, "$(Sys)$(Dev)Alrm1:Type-Sts") 
	field(LNK3, "$(Sys)$(Dev)Sys:Alrm-Sts") 
	field(LNK4, "$(Sys)$(Dev)Alrm1:HiT-RB") 
	field(LNK5, "$(Sys)$(Dev)Alrm1:LoT-RB") 
	field(LNK6, "$(Sys)$(Dev)LED-Sts")
}

